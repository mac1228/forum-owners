<div class="container-fluid content">			
	<div class="col-md-6 colsep">
		<h1 class="page-header">Forum Owner of the Week</h1>
		<h2>Feb 2 2015 - Feb 9 2015</h2>
		<h2><strong>Michael</strong></h2>
	</div>		
	<div class="col-md-6 colsep">
		<h1 class="page-header">Schedule</h1>
		<table class="table table-striped table-hover table-responsive">
	      <thead>
	        <tr>
	          <th>Date</th>
	          <th>Player Name</th>
	        </tr>
	      </thead>
	      <tbody>
	      	<% weeks.forEach(function(item){ %>
	      		<tr>
	      			<th scope="row"><%= item.Rotation._ %></th>
	      			<td><%= item.PlayerName._ %></td>
	      		</tr>
	      	<% }); %>
	      </tbody>
	    </table>		
	</div>			
</div>