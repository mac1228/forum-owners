<div class="container-fluid content">		
	<div class="col-md-8 rank">
		<div class="row">
			<h1>Leaders: <div class="timeline">Today</div></h1>
		</div>
		<div class="row">
			<table class="table table-striped table-hover table-responsive">
		      <thead>
		        <tr>
		          <th>Rank</th>
		          <th>Player Name</th>
		          <th>Points</th>
		        </tr>
		      </thead>
		      <tbody>
		      	<% for(var i = today.length-1; i >= 0; i--){ %>
		      		<tr>
		      			<th scope="row"><%= today.length - i %></th>
		      			<td><%= today[i].RowKey._ %></td>
		      			<td><%= today[i].Points._ %></td>
		      		</tr>
		      	<% } %>
		      </tbody>
		    </table>		
		</div>
	</div>
