<div class="container-fluid content">		
	<div class="col-md-8">
		<div class="row">
			<h1>Leaders: This Week</h1>
		</div>
		<div class="row">
			<table class="table table-striped table-hover table-responsive">
		      <thead>
		        <tr>
		          <th>Rank</th>
		          <th>Player Name</th>
		          <th>Points</th>
		        </tr>
		      </thead>
		      <tbody>
		      	<% for(var i = 1; i < players.length; i++){ %>
		      		<tr>
		      			<th scope="row"><%= i %></th>
		      			<td><%= players[i].PartitionKey._ %></td>
		      			<td>20</td>
		      		</tr>
		      	<% } %>
		      </tbody>
		    </table>		
		</div>
	</div>
	<div class="col-md-4">
		<div class="row myAffix leaderboardSelection">
			<ul class="nav nav-tabs">
			  <li role="presentation" class="active"><a href="#">Stack Overflow</a></li>
			  <li role="presentation"><a href="#">MSDN</a></li>
			  <li role="presentation"><a href="#">Email</a></li>
			</ul>
			<ul class="nav nav-pills nav-stacked">
			  <li role="presentation" class="active"><a href="#">Week</a></li>
			  <li role="presentation"><a href="#">Month</a></li>
			  <li role="presentation"><a href="#">Year</a></li>
			  <li role="presentation"><a href="#">All-Time</a></li>
			</ul>
		</div>
	</div>						
</div>