<div class="container-fluid content">			
	<div class="col-md-12">
		<div class="row">
			<h1 class="page-header">Stack Overflow</h1>
			<table class="table table-striped table-hover table-responsive">
		      <thead>
		        <tr>
		          <th>Tag</th>
		          <th>Link</th>
		        </tr>
		      </thead>
		      <tbody>
		      	<% stackoverflow.forEach(function(item){ %>
		      		<tr>
		      			<th scope="row"><%= item.name %></th>
		      			<td><a href="<%= item.url %>"><%= item.url %></a></td>
		      		</tr>
		      	<% }); %>
		      </tbody>
		    </table>
		</div>
		<div class="row">
			<h1 class="page-header">MSDN</h1>
			<table class="table table-striped table-hover table-responsive">
		      <thead>
		        <tr>
		          <th>Tag</th>
		          <th>Link</th>
		        </tr>
		      </thead>
		      <tbody>
		      	<% msdn.forEach(function(item){ %>
		      		<tr>
		      			<th scope="row"><%= item.name %></th>
		      			<td><a href="<%= item.url %>"><%= item.url %></a></td>
		      		</tr>
		      	<% }); %>
		      </tbody>
		    </table>
		</div>
		<div class="row">
			<h1 class="page-header">Optional</h1>
			<table class="table table-striped table-hover table-responsive">
		      <thead>
		        <tr>
		          <th>Name</th>
		          <th></th>
		        </tr>
		      </thead>
		      <tbody>
		      	<% optional.forEach(function(item){ %>
		      		<tr>
		      			<th scope="row"><%= item.name %></th>
		      			<td><a href="<%= item.url %>"><%= item.url %></a></td>
		      		</tr>
		      	<% }); %>
		      </tbody>
		    </table>
		</div>
	</div>			
</div>