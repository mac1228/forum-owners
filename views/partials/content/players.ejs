<div class="container-fluid content">			
	<h1>Player List</h1>	
	<table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>#</th>
          <th>Player Name</th>
          <th>StackOverlfow</th>
          <th>MSDN</th>
          <th>Microsoft Alias</th>
        </tr>
      </thead>
      <tbody>
      	<% var i = 0; %>
      	<% players.forEach(function(item){ %>
      		<tr>
      			<th scope="row">
              <div id="playerlist">
                <img class="avatar" src='<%= avatars[i] %>'>
              </div>
            </th>
      			<td class="verticalmiddle"><%= item.PartitionKey._ %></td>
      			<td class="verticalmiddle"><%=item.Stackoverflow._%></td>
      			<td class="verticalmiddle"><%=item.MSDN._%></td>
      			<td class="verticalmiddle"><%=item.Alias._%></td>
      		</tr>
      		<% i++ %>
      	<% }); %>
      </tbody>
    </table>		
</div>