<div class="container-fluid content">			
	<h1>Player List</h1>	
	<table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>#</th>
          <th>Player Name</th>
          <th>StackOverlfow</th>
          <th>MSDN</th>
          <th>Microsoft Alias</th>
        </tr>
      </thead>
      <tbody>
      	<% var i = 1; %>
      	<% players.forEach(function(item){ %>
      		<tr>
      			<th scope="row"><%= i %></th>
      			<td><%= item.PartitionKey._ %></td>
      			<td><%=item.Stackoverflow._%></td>
      			<td><%=item.MSDN._%></td>
      			<td><%=item.Alias._%></td>
      		</tr>
      		<% i++ %>
      	<% }); %>
      </tbody>
    </table>		
</div>