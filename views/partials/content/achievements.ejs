<div class="container-fluid content">			
	<div class="col-md-12">	
		<h1>Achievements</h1>		
		<table class="table table-striped table-hover table-responsive">
	      <thead>
	        <tr>
	          <th>Badge</th>
	          <th>Achievement</th>
	          <th>Requirement</th>
	        </tr>
	      </thead>
	      <tbody>
	      	<% achievements.forEach(function(item){ %>
	      		<tr>
	      			<th scope="row"><div class="achievementlistbadge"><img src="<%= item.url %>"></div></th>
	      			<td><%= item.name %></td>
	      			<td><%= item.req %></td>
	      		</tr>
	      	<% }); %>
	      </tbody>
	    </table>
    </div>					
</div>